{
    "name": "up2top/flat-files",
    "description": "Laravel flat files manager.",
    "authors": [
        {
            "name": "up2top",
            "email": "up2top-ru@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17"
    },
    "autoload": {
        "psr-4": {
            "up2top\\FlatFiles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "up2top\\FlatFiles\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "up2top\\FlatFiles\\FlatFilesServiceProvider"
            ]
        }
    }
}
